  <h1 style="position: relative;left: 15vh;font-size:4rem">Personal Details</h1>
            
  <table class="table " style="width:inherit; background-color:#333; color: white;height: 70vh "  >
    
    <tbody>
      <tr>
        <td>ELIGIBILITY NO </td>
        <td>:{{docs1.eno}}</td>
        
      </tr>
      <tr>
        <td>NAME </td>
        <td>:{{docs1.name}}</td>
        
      </tr>
      <tr>
        <td>MOTHER's Name </td>
        <td>:{{docs1.mName}}</td>
       
      </tr>
      <tr>
        <td>BIRTH Date </td>
        <td>:{{docs1.dob}}</td>
        
      </tr>
      <tr>
        <td>PERMANENT ADDRESS </td>
        <td>:{{docs1.address}}</td>
        
      </tr>
      <tr>
        <td>STATE </td>
        <td>:{{docs1.state}}</td>
        
      </tr>
      <tr>
        <td>COLLEGE </td>
        <td>:{{docs1.college}}</td>
        
      </tr>
      <tr>
        <td>PHONE NO </td>
        <td>:{{docs1.phoneNo}}</td>
        
      </tr>
      <tr>
        <td>Gender </td>
        <td>:{{docs1.gender}}</td>
        
      </tr>
      <tr>
        <td>EMAIL-ID </td>
        <td>:{{docs1.emailid}}</td>
        
      </tr>
     
    </tbody>
  </table>



</div>
<div class="col-sm-4">

 
