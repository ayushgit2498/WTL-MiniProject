    <tbody>
      <tr>
        <td>COURSE NAME </td>
        <td>:{{docs1.dept}}</td>
        
      </tr>
      <tr>
        <td>YEAR OF STUDY :</td>
        <td>:{{docs1.year}}</td>
        
      </tr>
      <tr>
        <td>Semester </td>
        <td>:{{docs1.sem}}</td>
       
      </tr>
     <tr>
        <td>SEAT-NO </td>
        <td>:{{docs1.sno}}</td>
        
      </tr>
     
    </tbody>
  </table>



  </div>
