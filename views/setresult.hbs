      <tr>
        <td>{{sName5}}</td>
        <input type="hidden" class="form-control" name="sn5" value={{sName5}}>   
        <td> <input type="text" class="form-control" name="sMarks5"></td>
        
      </tr>
    </tbody>
  </table>
 

 
   </div>
</div>
<br>
</div>
<div style="text-align: center;">
    <button type="submit" class="button" id="btnPrint" style=" background-color: #4CAF50;
  border: none;
