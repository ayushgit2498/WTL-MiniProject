                                     <button class="btn btn-primary btn-lg" type="submit">Logout</button>
                                    </form>
                                    
                            </ul>
                    </div>
        
    </nav>
</div>
<div class="container" id="outerdiv" >
  <div >
    {{#if filled}}
  <form id="srtup" action="/kr/admin/resultstore" method="post">
    {{else}}
  <form id="srtup" action="/kr/admin/result" method="post">
    {{/if}}
    <div class="form-group">
      <p style="font-size:3rem">Department</p>
      <select class="form-control" name="dept" style="height:3vh;">
        <option value="Comp">Computer Engineering</option>
        <option value="Entc">EnTc Engineering</option>
        <option value="IT">IT Engineering</option>
      </select>
      <br>
      <div class="form-group">
      <p style="font-size:3rem">Semester</p>
      <select class="form-control" name="sem" style="height:3vh;">
        <option value=1>Sem 1</option>
        <option value=2>Sem 2</option>
        <option value=3>Sem 3</option>
        <option value=4>Sem 4</option>
        <option value=5>Sem 5</option>
        <option value=6>Sem 6</option>
        <option value=7>Sem 7</option>
        <option value=8>Sem 8</option>
      </select>
      <br>
       
    <br>
    <div style="text-align: center;">
      <button  type="submit" class="button" id="btnPrint1" style=" background-color: #4CAF50;
    border: none;
    color: white;
    padding: 15px 25px;
    text-align: center;
    font-size: 16px;
    cursor: pointer;">Fetch Subjects</button> 
  </div>
    </div>
    <br>
  </div>  
  {{#if filled}}
		 <div id="dvContainer" >
		 	<br><br>
<div class="container" style="border-style: double; padding-top: 50px">
