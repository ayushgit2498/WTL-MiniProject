      <tr>
        <th>SUBJECT</th>
        <th>MARKS</th>
        
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>{{sName1}}</td>
        <input type="hidden" class="form-control" name="sn1" value={{sName1}}>   
        <td> <input type="text" class="form-control" name="sMarks1"></td>
        
      </tr>
      <tr>
        <td>{{sName2}}</td>
        <input type="hidden" class="form-control" name="sn2" value={{sName2}}>   
        <td> <input type="text" class="form-control" name="sMarks2"></td>
        
      </tr>
      <tr>
        <td>{{sName3}}</td>
