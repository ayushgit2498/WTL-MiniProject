<html>
	<head>
		
		<link rel="stylesheet" href="/css/timetable.css">
		<link rel="stylesheet" type="text/css" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css">
		<style>
 			.imageedit{
        	height: 100%;
        	width:100%;
        }
        .li1{
            font-size: large;
            position: relative;
            font-weight: bold;
        }
      
        .li1 a:hover{
        	text-decoration: none;
            border-bottom: 3px solid cyan;
            color: black;
            padding-bottom:0.5vh; 
        }
         .footer1{
            position: fixed;
            bottom: 0vh;
            left:0vh;
            height:6vh;
            width: 100%;
            background-color: black;
            color: white;
        }
        td{
          font-size: 2.5rem;
        }
		</style>

	</head>
	<body bgcolor="#00BCD4">
		
<div class="container-fluid">
        <nav class="navbar ">
            
                    <div class="row  col"  style="border-bottom:3px solid grey;">
                            <div class="col-1">
                                <img src="/assets/imgs/logo.jpg" class="img-fluid" style="height: 5rem;">
                            </div>
                                
                                 <ul class="d-flex   col align-items-end "  style="list-style-type: none;">  
                                    <div class="col d-flex justify-content-around">
																		<li class="li1 li2"><a href="/examdash">EXAMDASH</a></li>
                                    <li class="li1 li2"><a href="/kr/client/result">RESULTS</a></li>
                                    <li class="li1 li2"><a href="/kr/timetable">TIMETABLE</a></li>
                                    <li class="li1 li2"><a href="/maintenance">CONTACT</a></li>
                                    <li class="li1 li2"><a href="/maintenance">ABOUT US</a></li>
                                        </div>
                                         <button class="btn btn-primary btn-md">Logout</button>
                                        
                                </ul>
                        </div>
            
        </nav>
    </div>
		<div id="outline">
			<div class="header">
				<form action="/kr/timetable" method="post">
					<div class="form-box">
						<select name="dept" class="search-field course">
 				 			<div>
    							<option value="Comp">Computer Engineering</option>
   								<option value="EnTc">EnTc Engineering</option>
   								<option value="IT">IT Engineering</option>
   							</div>	 
 						 </select>
 						 <select name="sem" class="search-field location">
 				 			<div>
    							<option value=1>Sem One</option>
   								<option value=2>Sem Two</option>
   								<option value=3>Sem Three</option>
   								<option value=4>Sem Four</option>
   								<option value=5>Sem Five</option>
   								<option value=6>Sem Six</option>
   								<option value=7>Sem Seven</option>
   								<option value=8>Sem Eight</option>
   							</div>	 
 						 </select>
					<button class="search-btn" type="submit">Search</button>
						
					</div>

				</form>
			</div>
				<br><br><br>
				{{#if filled}}
				<table>
				  <tr>
				    <th>Subject Name</th>
				    <th>Date</th>
				    <th>Time</th>
				  </tr>
				  <tr>
				    <td>{{temp.sub1.sName}}</td>
				    <td>{{temp.sub1.dexam}}</td>
				    <td>10:00 am</td>
				  </tr>
 					<tr>
				    <td>{{temp.sub2.sName}}</td>
				    <td>{{temp.sub2.dexam}}</td>
				    <td>10:00 am</td>
				  </tr>			
					 <tr>
				    <td>{{temp.sub3.sName}}</td>
				    <td>{{temp.sub3.dexam}}</td>
				    <td>10:00 am</td>
				  </tr>
					 <tr>
				    <td>{{temp.sub4.sName}}</td>
				    <td>{{temp.sub4.dexam}}</td>
				    <td>10:00 am</td>
				  </tr>
					 <tr>
				    <td>{{temp.sub5.sName}}</td>
				    <td>{{temp.sub5.dexam}}</td>
				    <td>10:00 am</td>
				  </tr>	  
				</table>

	{{/if}}
		</div>
		</header>


	</body>

</html>